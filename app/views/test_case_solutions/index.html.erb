<p id="notice"><%= notice %></p>

<h1>Test Case Solutions</h1>

<table>
  <thead>
    <tr>
      <th>Solution</th>
      <th>Test case</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_case_solutions.each do |test_case_solution| %>
      <tr>
        <td><%= test_case_solution.solution %></td>
        <td><%= test_case_solution.test_case %></td>
        <td><%= test_case_solution.status %></td>
        <td><%= link_to 'Show', test_case_solution %></td>
        <td><%= link_to 'Edit', edit_test_case_solution_path(test_case_solution) %></td>
        <td><%= link_to 'Destroy', test_case_solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test Case Solution', new_test_case_solution_path %>
